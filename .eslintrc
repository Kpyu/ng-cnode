{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": [2, "never"],
    "generator-star":0,
    "prefer-const": 0,
    "no-var": 0,
    "no-unused-vars":0,
    "no-loop-func":0,
    "prefer-arrow-callback": 0,
    "func-names": 0,
    "space-before-function-paren": 0,
    "prefer-template": 0,
    "no-console": 0,
    "object-shorthand": 0,
    "no-param-reassign":0
  }
} 