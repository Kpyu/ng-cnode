{
  "extends": "airbnb",
  "rules": {
    "comma-dangle": [2, "never"],
    "prefer-const": 0,
    "no-var": 0,
    "quote-props":0,
    "prefer-arrow-callback": 0,
    "func-names": 0,
    "global-require":0,
    "space-before-function-paren": 0,
    "no-underscore-dangle":0,
    "prefer-template": 0,
    "no-console": 0,
    "object-shorthand": 0,
    "no-param-reassign":0,
    "import/no-unresolved":0
  }
} 